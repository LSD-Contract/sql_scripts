-- SCHEMA: thesaurus

-- DROP SCHEMA IF EXISTS thesaurus ;

CREATE SCHEMA IF NOT EXISTS thesaurus
    AUTHORIZATION postgres;

SELECT * FROM thesaurus."Categories" ORDER BY id ASC
